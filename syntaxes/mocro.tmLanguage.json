{
  "scopeName": "source.mocro",
  "patterns": [
    {
      "name": "comment.line.double-dash.mocro",
      "match": "--.*$"
    },
    {
      "name": "keyword.control.mocro",
      "match": "\\b(if|then|else|elseif|end|while|do|for|function|return|local)\\b"
    },
    {
      "name": "constant.numeric.mocro",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "string.quoted.double.mocro",
      "begin": "\"",
      "end": "\""
    }
  ],
  "repository": {}
}
